pipeline (
    agent any
    stages (
    
       stage("checkout") {
       
          steps {
              echo 'this is checkout'
          
          }
       }    
       stage("build") {
       
          steps {
              echo 'this is build'
          
          }
       }
       stage("deploy") {
       
          steps {
              echo 'this is deploy'
          
          }
       }
    }
)
